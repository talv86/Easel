{
  "name": "talvbansal/easel",
  "description": "A minimal blogging package for laravel",
  "keywords": ["blog", "blogging", "laravel", "easel", "markdown", "developers", "minimal"],
  "license": "MIT",
  "authors": [
    {
      "name": "Talvinder Bansal",
      "email": "talvbansal@hotmail.com",
      "role": "Developer"
    }
  ],

  "require": {
    "php": "^7.1.3",
    "bestmomo/scafold": "dev-master",
    "dflydev/apache-mime-types": "^1.0",
    "doctrine/dbal": "^2.5",
    "erusev/parsedown": "^1.6",
    "proengsoft/laravel-jsvalidation": "^2.0",
    "teamtnt/laravel-scout-tntsearch-driver": "^3.0",
    "spatie/laravel-backup": "^5.0",
    "talvbansal/media-manager": "~2.5"
  },
  "require-dev": {
    "fzaninotto/faker": "^1.4",
    "mockery/mockery": "^0.9",
    "phpunit/phpunit": "^7.0",
    "orchestra/testbench": "^3.6",
    "orchestra/testbench-browser-kit": "^3.6"
  },
  "autoload": {
    "psr-4": {
      "Easel\\": "src/"
    },
    "files": [
      "src/Helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "EaselTest\\": "tests/"
    },
    "classmap": [
      "tests/TestCase.php"
    ]
  },

  "extra": {
    "laravel": {
      "providers": [
        "Easel\\Providers\\EaselServiceProvider"
      ]
    }
  },

  "minimum-stability": "dev",
  "config": {
    "preferred-install": "source",
    "sort-packages": true
  }
}

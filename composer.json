{
  "name": "talvbansal/easel",
  "description": "A minimal blogging package for laravel",
  "keywords": ["blog", "blogging", "laravel", "easel", "markdown", "developers", "minimal"],
  "license": "MIT",
  "authors": [
    {
      "name": "Talvinder Bansal",
      "email": "talvbansal@hotmail.com",
      "role": "Developer"
    }
  ],

  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/reganjohnson/laravel-jsvalidation"
    }
  ],

  "require": {
    "php": ">=7.0",
    "bestmomo/scafold": "dev-master",
    "dflydev/apache-mime-types": "^1.0",
    "doctrine/dbal": "^2.5",
    "erusev/parsedown": "^1.6",
    "proengsoft/laravel-jsvalidation": "^1.6.0-dev",
    "teamtnt/laravel-scout-tntsearch-driver": "2.*",
    "spatie/laravel-backup": "^3.10",
    "talvbansal/media-manager": "dev-master"
  },
  "require-dev": {
    "symfony/dom-crawler": "3.*",
    "symfony/css-selector": "3.*",
    "fzaninotto/faker": "~1.4",
    "mockery/mockery": "0.9.*",
    "phpunit/phpunit": "~5.0",
    "orchestra/testbench": "~3.0",
    "symfony/browser-kit": "^3.2",
    "orchestra/testbench-browser-kit": "~3.1"
  },
  "autoload": {
    "psr-4": {
      "Easel\\": "src/"
    },
    "files": [
      "src/Helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "EaselTest\\": "tests/"
    },
    "classmap": [
      "tests/TestCase.php"
    ]
  },
  "minimum-stability": "dev",
  "config": {
    "preferred-install": "source",
    "sort-packages": true
  }
}
